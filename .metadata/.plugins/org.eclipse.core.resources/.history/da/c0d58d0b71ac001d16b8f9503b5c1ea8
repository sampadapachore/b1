package com.testing;

public class addarray {
	public static void main(String[] args) {
	int [] x= {10,20,30,40,50};
		int sum=0;
		int digit;
		int result=0;
		for(int i=0;i<=4;i++) {
			result=result+x[i];
		}
		System.out.println("result:"+result);
		while(result>0) {
			digit=result%10;
			result=result/10;
			sum=sum+digit;
		}
		System.out.println("sum:"+sum);
		}
		}
		

	